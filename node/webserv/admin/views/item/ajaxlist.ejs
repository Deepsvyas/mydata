   <table id="example1" class="table table-bordered table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th class="" >Sr. No.</th>
                                        <th class="" >Item Name</th>
                                        <th class="" >Shop Name</th>
                                        <th class="" >Category</th>
                                        <th class="" >Price</th>
                                        <th class="" >Quantity</th>
                                        <th class="" >Description</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>


                                    <% if(data.length){ 

                                    for(var i = 0;i < data.length;i++) { %>

                                    <tr>
                                        <td> <%=i+1%> </td>
                                        <td><%=data[i].item_name%></td>
                                        <td><%=data[i].shop_id%></td>
                                        <td><%=data[i].category_id%></td>
                                        <td><%=data[i].price%></td>
                                        <td><%=data[i].available_qty%></td>
                                        <td><%=data[i].description%></td>
                                        <td>
                                            <a class="btn btn-success btn-sm a-inside editItem" data-id="<%=data[i]._id%>">Edit</a>
                                            <a class="btn btn-danger btn-sm a-inside deleteItem" data-id="<%=data[i]._id%>">Delete</a>
                                        </td>
                                    </tr>
                                    <% }

                                    }else{ %>
                                    <tr>
                                        <td colspan="3">No user</td>
                                    </tr>
                                    <% } %>



                                </tbody>

                            </table>